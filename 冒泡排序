public class MaoPao {
	public static void main(String[] args) {
		int[] a={6,4,3,9,1,8,5};
		maoPaoSort(a);
		
	}
	private static void maoPaoSort(int[] a) {
		for(int i=0;i<a.length-1;i++){
			for(int j=0;j<a.length-1-i;j++){
				int temp;
				if(a[j]>a[j+1]){
					/*a[j]=a[j]+a[j+1];
					a[j+1]=a[j]-a[j+1];
					a[j]=a[j]-a[j+1];
					*/
					temp=a[j];
					a[j]=a[j+1];
					a[j+1]=temp;
				}
			}
		}
		for(int i=0;i<a.length;i++){
			System.out.print(a[i]+",");
		}
	}
}
